GUI --- "1"Buffer
GUI --"1" Invoker

Receiver "1"- Command
Receiver -down-"1" Buffer
Receiver --down"1" Clipboard

SimpleGUI -right|> GUI

Engine -right-|> Receiver

Invoker "1"--left Command

Copy -- Originator
Paste -- Originator
Replace -- Originator
Delete -- Originator
Cut -- Originator
Insert -- Originator
Undo -- Originator
Redo -- Originator
Script -- Originator
Load -- Originator

CareTaker -up- Originator
Memento -left- CareTaker

Script - CareTaker

Copy -up-|> Command
Paste -up-|> Command
Replace --up-|> Command
Delete -up-|> Command
Cut --up-|> Command
Insert -up--|> Command
Undo -up-|> Command
Redo -up--|> Command
Script -up-|> Command
Load -up-|> Command

SimpleBuffer -up-|> Buffer
SimpleClipboard -up-|> Clipboard

DummyInvoker -left|> Invoker

MyOriginator -|> Originator
StackCareTaker -|> CareTaker
