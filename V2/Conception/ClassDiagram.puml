    - receiver
    + execute()
}

class Delete #ff6961 {
    - int position
    - receiver
    + execute()
}

class Insert #ff6961 {
    + int position
    - String insert
    - receiver
    + execute()
}

class Undo #ff6961 {
    - receiver
    + execute()
}

class Redo #ff6961 {
    - receiver
    + execute()
}

class Script #ff6961 {
    - receiver
    + execute()
}

class Load #ff6961 {
    - receiver
    + execute()
}

class SimpleBuffer #ff6961;line.bold:C80900 {
    - String content
    + getInstance()
    + addContent(String s, int position)
